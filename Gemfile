# If you duplicate your repository from year to year, feel free to keep your gems in additional groups here
# Example:
group '2020' do
  gem 'rgl', require: false # Graph logic - https://github.com/monora/rgl
  gem 'rltk', require: false # Language-building - https://github.com/chriswailes/RLTK
end

group '2021' do
  # Z3 4.8 prevents us from using Optimize
  gem 'z3', github: 'taw/z3', ref: '74302129029aadcd7c493a3577a3af3427334667'

  gem 'nokogiri'
end
